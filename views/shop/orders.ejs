<%- include('../includes/head.ejs') %>
</head>
<body>
  <%- include('../includes/navigation.ejs') %>
  <main>
    <% if (orders.length <= 0) { %>
      <h1>Nothing here...</h1>
    <% } else { %>
      <ul>
        <% orders.forEach(order => { %>
          <li>
            <h4># <%= order._id %></h4>
            <ul>
              <% order.products.forEach(item => { %>
                <li>
                  <p><%= item.product.title %>(<%= item.quantity %>): $ <%= item.product.price %></p>
                </li>
              <% }) %>
            </ul>
          </li>
        <% }) %>
      </ul>
  <% } %>
  </main>
<%- include('../includes/end.ejs') %>